  local real AO8
  local real AP8
  local real d
  local real AJ8=(LoadReal(hashtable019,(Cache),(44)))
  local real rate=AJ8*.02
  local group g
  local integer Count=(LoadInteger(hashtable019,(Cache),(34)))
  if S4E!=null then
    set TargetX=GetUnitX(S4E)
