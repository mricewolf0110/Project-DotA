  endif
  if IsUnitType(u,UNIT_TYPE_HERO)then
    if GetUnitAbilityLevel(u,'A021')==0 then
      call UnitAddAbility(u,'A021')
    endif
  else
    if GetUnitAbilityLevel(u,'A29C')==0 then
      call UnitAddAbility(u,'A29C')
