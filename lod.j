    endif
    call LoDMoonGlaive_BounceFrom(Source,Target,Damage/GetUnitPhysicalResistance_Result,AlreadyHit,Level)
  else
    //call echo(GetUnitName(Target)+" hit for "+R2S(GetEventDamage()))
  endif
  set t=null
  set Source=null
  set Target=null
  set AlreadyHit=null
  return false
