    set loopA = loopA + 1
  endloop
  
  call SaveInteger(MHT,info,0,8)
  call SaveInteger(MHT,info,1,level*5)
  call SaveInteger(MHT,info,2,10 + 15*level)
  call SaveReal(MHT,info,0,x)
