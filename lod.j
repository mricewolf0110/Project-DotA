  if GetUnitAbilityLevel(u,id_b)==0 and(IsUnitType(u,UNIT_TYPE_STRUCTURE)==true and GetUnitAbilityLevel(u,LoadInteger(hashtable019,GetHandleId(LoadTimerHandle(hashtable019,GetHandleId(u),id_b)),0))==0)then
