      set i=i+1
    endloop
    return
  endif
  loop
    exitwhen i>24
    if i!=amount/4 and GetUnitAbilityLevel(Hero,BerserkerBloodResistCont[i])>0 then
      call UnitRemoveAbility(Hero,BerserkerBloodResistCont[i])
