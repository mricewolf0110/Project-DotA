  if b_isPickTime==true then
    if TimerGetElapsed(GlobalTimer)>ResetCooldown[PlayerId]+4 then
      if i<7 and i>0 then
        //local integer i=(SelectedSkillHeroPointValue[PlayerId]*4)-4
        set ResetCooldown[PlayerId]=TimerGetElapsed(GlobalTimer)
