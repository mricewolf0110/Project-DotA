  endif
  if i==1 then
    loop
      call SetPlayerTechMaxAllowed(p,RangeHeroesArray[k],0)
      set k=k+1
      exitwhen k>RangeHeroesAmount
    endloop
  elseif i==2 then
    loop
      call SetPlayerTechMaxAllowed(p,MeleeHeroesArray[k],0)
      set k=k+1
      exitwhen k>MeleeHeroesAmount
    endloop
  endif
endfunction

