      if IsIndexIdCharged(indexId)then
        set consumable=true
      else
        set consumable=false
      endif
      if(GetPlayerSlotState((owner))==PLAYER_SLOT_STATE_LEFT)and Bottle_IsIndexRune(indexId)then
      else
        call DestroyItem(Item)
