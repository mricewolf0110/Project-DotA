      //i dont know ~.~
    endif
  else
    if time>=limit then
      call DestroyEffect(LoadEffectHandle(MHT,hu,'A002'+1))
      call FlushChildHashtable(hashtable019,h)
      call DestroyTrigger(t)
      call RemoveSavedHandle(MHT,hu,'A002')
