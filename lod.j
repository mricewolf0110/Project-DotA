    call SaveReal(hashtable019,(Cache),(6),((x1)*1.))
    call SaveReal(hashtable019,(Cache),(7),((y1)*1.))
    call SaveInteger(hashtable019,(Cache),(5),(Level))
    call TriggerRegisterTimerEvent(t,1,true)
