function GetAghaVersion takes integer id returns integer
  local integer i=1
  loop
    exitwhen i>AghaCounter
    if id==AghaUpgradedSpellID[i] or id==AghaBaseSpellID[i]  then
      return i
    endif
    
    set i=i+1
  endloop
  return 0
endfunction

