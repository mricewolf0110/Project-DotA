    call SaveReal(hashtable019,(Cache),(7),((TargetY)*1.))
  endif
  set a=AngleBetweenXY(NHE,NIE,TargetX,TargetY)*bj_DEGTORAD
  set d=DistanceBetweenXY(NHE,NIE,TargetX,TargetY)
  if GetTriggerEventId()==EVENT_WIDGET_DEATH then
