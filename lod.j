function Lod_IceWall_GetSlow takes unit Source returns integer
  local integer Level=GetUnitAbilityLevel(Source,'Z606')+1
  if Level==1 then
    return 'A230'
  elseif Level==2 then
    return 'A22Z'
  elseif Level==3 then
    return 'A22Y'
  elseif Level==4 then
    return 'A22U'
  elseif Level==5 then
    return 'A22V'
  elseif Level==6 then
    return 'A22W'
  elseif Level==7 then
    return 'A22X'
  endif
  return 'A230'
endfunction

