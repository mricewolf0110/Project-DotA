    elseif GetTriggerEventId()==EVENT_PLAYER_UNIT_ISSUED_POINT_ORDER then
      call SaveInteger(hashtable019,GetHandleId(u),'0ORD',GetIssuedOrderId())
      call SaveInteger(hashtable019,GetHandleId(u),'ORDT',2)
