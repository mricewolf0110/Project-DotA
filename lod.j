        set g=GetAvailableGroup()
        set TEMPUNIT=Source
        call GroupEnumUnitsInRange(g,GetUnitX(Source),GetUnitY(Source),1400,Condition(function Ignite_PossibleTarget))
        call GroupRemoveUnit(g,CWE)
