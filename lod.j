  //call BJDebugMsg("xin: "+Id2String('M106'))
  //call BJDebugMsg("morfid: "+Id2String(morph))
  //call BJDebugMsg(B2S(morph!='M106'))
  
  if morph!='M106'then //prevents the unit from transforming into itself \:3/
