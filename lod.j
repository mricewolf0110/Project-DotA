function GetAghaIdByBase takes integer id returns integer
  local integer i=1
  local integer k=0
  loop
    if AghaBaseSpellID[i]==id then
      set k=i
    endif
    set i=i+1
    exitwhen k>0 or i>AghaCounter
  endloop
  return k
endfunction

function C79 takes nothing returns boolean
