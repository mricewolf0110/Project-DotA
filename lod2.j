  loop
    exitwhen PlayerId>11
    set Hero=udg_Hero[PlayerId]
    if Hero!=null and IsDead(Hero)==false and GetUnitTypeId(Hero)==HeroTypeId[PlayerId] and IsHexed(Hero)==false then
