      call CourierReissueDeliveryXY(GetHandleId(t))
      call SaveCourierState(Courier,"Deliver to "+udg_Colors[GetPlayerId(GetOwningPlayer(Hero))]+PlayerNames[GetPlayerId(GetOwningPlayer(Hero))]+"|r")
