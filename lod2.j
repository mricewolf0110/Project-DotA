    call RemoveSavedHandle(HY,GetHandleId(LoadUnitHandle(HY,h,0)),'EVAS')
    call DisableEvasionList(GetOwningPlayer(LoadUnitHandle(HY,h,0)),false)
    call PauseTimer(t)
