      endif
      call TextTagOnUnit(I2S(R2I(Damage))+"!",2,Target,.025,100,0,200,216)
    endif
  elseif(TimerGetElapsed(GlobalTimer))>W2G then
    call DestroyEffect((LoadEffectHandle(HY,h,(32))))
