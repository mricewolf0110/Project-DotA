      call CleanTrigger(t)
      call UnitRemoveAbility(Target,'A1W2')
      call UnitRemoveAbility(Target,'B0DO')
    endif
  else
    set Count=Count+1
    call SaveInteger(HY,h,(34),(Count))
    if GetUnitAbilityLevel(Target,'A1W2')==0 or Count==DP9 then
