    call DisplayTimedTextToForceEx(bj_FORCE_ALL_PLAYERS,25.,udg_Colors[GetPlayerId(GetTriggerPlayer())]+(PlayerNames[GetPlayerId((GetTriggerPlayer()))])+" ("+name+")|r|c00ff0303 "+GetObjectName('n02D')+"|r")
