function GetUnitOffsetSpeed takes unit u returns integer
  local integer lvl = GetUnitAbilityLevel(u,'EUL1') //euls
  local integer lvl2 = GetUnitAbilityLevel(u,'EUL2') //euls
  local integer movespeed
  if GetUnitAbilityLevel(u,'A33W')>0 then
    return 1000
  endif
  if lvl > 0 then
    if lvl==1 then
      set movespeed=40
    elseif lvl==2 then
      set movespeed=90
    elseif lvl==4 then
      set movespeed=95
    else
      set movespeed=100
    endif
  endif
  if lvl2>0 then
    if lvl2==1 then
      set movespeed=125
    elseif lvl2==2 then
      set movespeed=140
    endif
  endif
  if lvl+lvl2>0 then
    return movespeed
  endif
  if GetUnitAbilityLevel(u,'A00D')>0 then //travels
    return 100
