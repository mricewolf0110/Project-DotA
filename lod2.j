  local string s=I2S(R2I(delay))
  local real x=-7030
  local real y=6766
  if oh>1 then
    set im1=LoadImageHandle(HY,oh,1)
    set im2=LoadImageHandle(HY,oh,2)
    call DestroyImage(im1)
    call DestroyImage(im2)
    call PauseTimer(oldtimer)
    call DestroyTimer(oldtimer)
