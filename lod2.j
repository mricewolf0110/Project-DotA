      if GetUnitAcquireRange(u)!=0 and IsUnitType(u,UNIT_TYPE_STRUCTURE)then
        call TriggerRegisterUnitInRange(t,u,600,Condition(function ReturnTrue))
      endif
    endloop
    call KillGroup(g)
    call TriggerAddCondition(t,Condition(function ULD))
