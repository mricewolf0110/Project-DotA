      if Mode_MA then
        if i>0 then
          call UnitMakeAbilityPermanent(u,true,AghaUpgradedSpellID[i])
        endif
      endif
      if Q28>='A40K' and Q28<='A40N' then
        if GetUnitAbilityLevel(u,Q28+8)>0 then
          call SetUnitAbilityLevel(u,Q28+8,1)
