    set Time=I2S(MinutesForTime)+":0"+I2S(SecondsForTime)
  else
    set Time=I2S(MinutesForTime)+":"+I2S(SecondsForTime)
  endif
  if GameEnded==false then
    set LeftAt[GetPlayerId(GetTriggerPlayer())]="|c00555555"+Time+"|r"
