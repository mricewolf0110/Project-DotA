function SubStringTillSpace takes string s returns string
  local integer i=0
  local integer k=StringLength(s)
  local string ss=""
  local string s3
  loop
    set s3=SubString(s,i,i+1)
    if s3!=" " then
      set ss=ss+s3
    else
      return ss
    endif
    set i=i+1
    exitwhen i+1>k
  endloop
  return ss
endfunction

