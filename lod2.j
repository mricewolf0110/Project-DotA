  local integer charges=LoadInteger(HY,GetHandleId(u),'A064')
  //	call echo(I2S(charges))
  if charges < 3 then
    set time=time-0.2
    if time<=0.then
      call SaveInteger(HY,GetHandleId(u),'A064',charges+1)
      call SaveReal(HY,h,0,40.0)
    else
