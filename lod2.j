  else
    set JY[i]=null
  endif
endfunction

function DaI_Save takes destructable d returns integer
  set MY=MY+1
  if MY>24000 then
    set LY[MY-16000]=d
  elseif MY>8000 then
    set KY[MY-8000]=d
  else
    set JY[MY]=d
  endif
  return MY
