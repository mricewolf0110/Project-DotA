      call IssueTargetOrder(d,"soulburn",u)
      set d=null
    endif
  else
    if GetIssuedOrderId()>=852008 and GetIssuedOrderId()<=852013then
      call InterruptUnit(u)
    endif
  endif
  set u=null
  set t=null
endfunction

function STFU takes unit u, integer buffid, real dur returns nothing
