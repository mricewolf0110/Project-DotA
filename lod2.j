    call TriggerRegisterAnyUnitEvent(t,EVENT_PLAYER_UNIT_ATTACKED)
    call TriggerAddCondition(t,Condition(function U5D))
  endif
  call CleanTrigger(GetTriggeringTrigger())
  set t=null
  set g=null
  set u=null
  return false
endfunction

