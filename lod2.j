  local integer Level=(LoadInteger(HY,h,5))
  local real a=(LoadReal(HY,h,(13)))
  local unit Hero=(LoadUnitHandle(HY,h,(14)))
  local trigger RPE=(LoadTriggerHandle(HY,h,(11)))
