function GoldPerSecTrigger takes nothing returns boolean
  local integer W98=1
  local integer pid
  local integer i=1
  local real time=TimerGetElapsed(GlobalTimer)
  if Mode_EM then
    set W98=2
  endif
  loop
    exitwhen i==6
