  endif
  if GetTriggerEventId()==EVENT_UNIT_SPELL_EFFECT then
    if GetTriggerEvalCount(t)>1 and GetSpellAbilityId()=='A0S1' and IsBlocked(GetSpellTargetUnit())==false and Target==GetSpellTargetUnit()then
