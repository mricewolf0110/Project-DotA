      if(GetRandomInt(1,TF8)==1)then
        set TD8=ITEM_TYPE_ARTIFACT
        set TE8=T88
      endif
    endif
    set T88=T88+1
    exitwhen T88>10
  endloop
  if(TF8==0)then
    set TD8=null
    return
  endif
  call T48(TD8,TE8)
  set TD8=null
endfunction

