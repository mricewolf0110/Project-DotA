  call TriggerAddCondition(t,Condition(function HeroAttackedLogic_Attacked))
  set tm=CreateTimer()
  call SaveTimerHandle(HY,(GetHandleId(Hero)),(141),(tm))
  call SaveUnitHandle(HY,(GetHandleId(tm)),2,(Hero))
  set g=null
  set t=null
  set tm=null
