function OJD takes integer a returns integer
  local integer i=0
  local integer x=0
  local string s=I2S(a)
  loop
    exitwhen i>StringLength(s)
    if SubString(s,i,i+1)=="1"then
      set x=x+1
    endif
    set i=i+1
  endloop
  return x
endfunction

