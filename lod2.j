  set g=GetAvailableGroup()
  call SaveGroupHandle(HY,h,(144),(g))
  set t=CreateTrigger()
  call TriggerRegisterUnitEvent(t,Hero,EVENT_UNIT_ISSUED_TARGET_ORDER)
