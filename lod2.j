      call SetNapalmFactory(Source,0,0)
      call DestroyEffect(AddSpecialEffectTarget("Objects\\Spawnmodels\\Naga\\NagaDeath\\NagaDeath.mdl",Source,"origin"))
    endif
    call SaveReal(HY,h,412,dmgStored)
    call SaveReal(HY,h,411,time)
  endif
  set t=null
  set Source=null
  set Target=null
  return false
endfunction

