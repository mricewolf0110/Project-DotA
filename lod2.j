function Kodo_DrumsProvideBonusesStr takes unit u, integer str returns nothing
  local integer array b
  local integer a=str
  local integer c=1
  local integer i=0
  local integer z=5
  local real hp
  local integer array bonuses
  set bonuses[0]='A32N'
  set bonuses[1]='A32O'
  set bonuses[2]='A32P'
  set bonuses[3]='A32Q'
  set bonuses[4]='A32R'
  set bonuses[5]='A32S'
  if str<1 then
    set hp=GetWidgetLife(u)
    call UnitRemoveAbility(u,bonuses[0])
