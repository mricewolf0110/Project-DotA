    set a=GetAngleBetweenUnits(attacker,target)*bj_DEGTORAD
    if GetUnitAbilityLevel(attacker,'A0IM')==1 then
      set dmg=80
    elseif GetUnitAbilityLevel(attacker,'A0IQ')==1 then
