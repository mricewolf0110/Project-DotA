  endif
  if d<1 then
    set d=0
  endif
  set PKD=I2S(k)
  set PMD=I2S(d)
  set O3D=PKD+"/"+PMD
  if Source==GetLocalPlayer()then
    set O3D=ORANGE+PKD+"|r/"+ORANGE+PMD+"|r"
  endif
  return O3D
endfunction

