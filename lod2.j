      call DamageTarget(u,u2,1,burndmg)
      call DestroyEffect(AddSpecialEffectTarget("Abilities\\Spells\\Other\\ImmolationRed\\ImmolationRedDamage.mdl",u2,"chest"))
      call GroupRemoveUnit(gg,u2)
