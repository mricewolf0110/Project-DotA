    call PlayerSetLeaderboard(Scourges[i],MyLB[pid])
    call LeaderboardDisplay(MyLB[pid],false)
    call LeaderboardSetLabel(MyLB[pid],"Default string")
    
    set i=i+1
    exitwhen i>5
  endloop
  
endfunction

