      call SaveUnitHandle(HY,'0GTU',1488,u)
      call SaveInteger(HY,'0GLS',1488,Q28)
      set ExtraAbilityLevel[PlayerId*2+1]=2
      if HaveSavedString(MHT,Q28,1001) then
