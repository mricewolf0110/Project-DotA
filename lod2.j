  if GetUnitAbilityLevel(u,'A04R')==0 then
    return u
  endif
  set tt_unit1=WV7
  set g=GetAvailableGroup()
  call GroupEnumUnitsInRange(g,GetUnitX(u),GetUnitY(u),900,Condition(function DefaultEnemyFilter))
  set WU7=FirstOfGroup(g)
  call KillGroup(g)
  set g=null
  return WU7
endfunction

