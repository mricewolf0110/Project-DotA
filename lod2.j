  set bb=false
  loop
    if(GetPlayerController(Player(PlayersId[i]))==MAP_CONTROL_USER)and(GetPlayerSlotState(Player(PlayersId[i]))==PLAYER_SLOT_STATE_PLAYING)then
      set bb=true
      if not PlayerFFed[PlayersId[i]]then
        set b=false
        exitwhen true
      endif
    endif
    set i=i+1
    exitwhen i>11
  endloop
  if b and bb then
    call RF8(2)
  endif
endfunction

