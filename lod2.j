    endif
  else
    if LoadInteger(HY,h,1)==0 then
      if LoadBoolean(HY,h,0)==false then
        call FlushChildHashtable(HY,h)
        call CleanTrigger(t)
      else
        call SaveInteger(HY,h,1,1)
      endif
