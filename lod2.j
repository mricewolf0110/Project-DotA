  endif
  //if CTE!='A0MT' then
  call SaveInteger(HY,(Cache),'ORDT',CVE)
  call TriggerRegisterTimerEvent(t,0.3,true)
  call TriggerAddCondition(t,Condition(function CRE))
  //endif
